<div class="well">
    <h1>Details</h1>
    <h3><%= trailDetails.trails[0].name %></h3>
    <div><img src="<%= trailDetails.trails[0].imgMedium%>"></div>
    <p>Max Elevation: <%= trailDetails.trails[0].high%> feet</p>
    <p>Min Elevation: <%= trailDetails.trails[0].low%> feet</p>
    <a href="/trail/favorites">Back to favorites</a><br>
    <form method="POST" action="/trail/favorites<%=trailDetails.number%>/?_method=DELETE"><br>
        <button class="btn btn-primary" type="submit">Remove from Favorites</button>
        
    </form>
</div>